[Unit]
Description=Gnome gocryptfs automounter service
PartOf=graphical-session.target

[Service]
Type=oneshot
RemainAfterExit=yes
ExecStart=gnome-gocryptfs mount --auto
ExecStop=gnome-gocryptfs unmount

[Install]
WantedBy=graphical-session.target
